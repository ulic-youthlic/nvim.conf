{
    "neodev": {
        "library": {
            "enabled": true,
            "plugins": [
                "nvim-lspconfig",
                "lsp"
            ]
        }
    },
    "neoconf": {
        "plugins": {
            "lua_ls": {
                "enabled": true
            }
        }
    },
    "lspconfig": {
        "lua_ls": {
            "Lua.completion.callSnippet": "Replace"
        },
        "rust-analyzer": {
            "rust-analyzer.checkonsave": true,
            "rust-analyzer.check.command": "clippy"
        }
    }
}
